/*
NYC Evictions Data Exploration 
*/

--DATA CLEANING
--Determine if Court Index Number can be used as unique identifier in dataset via query to determine if more than one address is associated with each Court Index Number

SELECT 
Court_Index_Number, COUNT(DISTINCT Eviction_Address ) AS distinct_address_count
FROM `nyc-eviction-data.NYC_evictions_downloaded_20240102.NYC_evictions` 
GROUP BY
  Court_Index_Number
HAVING
  COUNT(DISTINCT Eviction_Address) > 1;
